package me.gamrboy4life.paradox.module.info;

import me.gamrboy4life.paradox.mods.impl.ModBuildingBlock1;
import me.gamrboy4life.paradox.module.Category;
import me.gamrboy4life.paradox.module.Module;

public class BuildingBlock1 extends Module {

    private ModBuildingBlock1 modBuildingBlock1;

    public BuildingBlock1() {
        super("建築ブロック1", 0, Category.INFO);
        modBuildingBlock1 = new ModBuildingBlock1();
    }

    @Override
    public void onUpdate() {
        modBuildingBlock1.setEnabled(this.isToggled());
    }
}